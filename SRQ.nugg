types: begin of ts_subadrc,
         addrnumber type ad_addrnum,
         date_from  type ad_date_fr,
         nation     type ad_nation,
         name       type ad_name1,
         street     type ad_street,
         house_no   type ad_hsnm1,
         city       type ad_city1,
         region     type regio,
         country    type land1,
       end of ts_subadrc,

       begin of ts_so_adr,
         sign   type tvarv_sign,
