  <method CLSNAME="ZCL_DYN_REMOTE" CMPNAME="GET_FIELD_VALUE" VERSION="1" LANGU="E" DESCRIPT="Get Field Value" EXPOSURE="2" STATE="1" EDITORDER="0 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" MTDNEWEXC="X" BCMTDCAT="00" BCMTDSYN="0">
